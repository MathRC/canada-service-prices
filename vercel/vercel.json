{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    },
    {
      "src": "assets/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "server.py"
    },
    {
      "src": "/assets/(?<path>.*)",
      "dest": "assets/<path>"
    }
  ]
}
